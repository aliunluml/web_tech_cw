<div class="protected_banner">
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">
        <div class="form-group">
          <form action='/logout' method='get'>
            <label for="logOutInput">Logged in as <%= username %></label>
            <input id="logOutInput" type="submit" value="Log Out" class = "sumbit_button" />
          </form>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-md protectedbtn" data-href="/">
        <svg width="60" height="60" xmlns="http://www.w3.org/2000/svg">
          <defs>
            <linearGradient id="gradient" x1="0%" y1="0%" x2="0%" y2="100%">
              <stop offset="0%" style="stop-color:rgb(0, 0, 0); stop-opacity:1" />
              <stop offset="100%" style="stop-color:rgb(150, 150, 150); stop-opacity:1" />
            </linearGradient>
          </defs>
          <path d=" M 10 20 l 0 35 l 40 0 l 0 -35 l -40 0 " fill="transparent" stroke="black"/>
          <path d=" M 15 55 l 0 -20 l 15 0 l 0 20 l -15 0 " fill="url(#gradient)"/>      # the door of the home
          <path d=" M 30 0 l -30 20 l 60 0 l -30 -20 " fill="url(#gradient)"/>           # the rooftop of the home
          Home Icon
        </svg>
        <strong>Home</strong>
      </div>
      <div class="col-md protectedbtn" data-href="/dashboard">
        <svg width="60" height="60" xmlns="http://www.w3.org/2000/svg">
          <defs>
            <linearGradient id="gradient" x1="0%" y1="0%" x2="0%" y2="100%">
              <stop offset="0%" style="stop-color:rgb(0, 0, 0); stop-opacity:1" />
              <stop offset="100%" style="stop-color:rgb(150, 150, 150); stop-opacity:1" />
            </linearGradient>
          </defs>
          <path d=" M 5 55 l 0 -15 l 40 -40 l 15 15 l -40 40 l -15 0 " fill="transparent" stroke="black"/>
          <path d=" M 10 45 l 40 -40 " fill="transparent" stroke="black"/>
          <path d=" M 15 50 l 40 -40 " fill="transparent" stroke="black"/>
          <path d=" M 5 55 l 0 -15 l 15 15 l -15 0 " fill="url(#gradient)"/>          # the end of the pencil
          <path d=" M 45 0 l 15 15 l -5 5 l -15 -15 l 5 -5 " fill="url(#gradient)"/>  # the eraser on the other end
          Dashboard Icon
        </svg>
        <strong>Dashboard</strong>
      </div>
      <div class="col-md protectedbtn" data-href="/corpus">
        <svg width="60" height="60" xmlns="http://www.w3.org/2000/svg">
          <defs>
            <linearGradient id="gradient" x1="0%" y1="0%" x2="0%" y2="100%">
              <stop offset="0%" style="stop-color:rgb(0, 0, 0); stop-opacity:1" />
              <stop offset="100%" style="stop-color:rgb(150, 150, 150); stop-opacity:1" />
            </linearGradient>
          </defs>
          <path d=" M 5 0 l 50 0 l 0 60 l -50 0 l 0 -60 " fill="transparent" stroke="black"/>
          <path d=" M 10 20 l 40 0 " fill="transparent" stroke="black"/>        # a line on the page
          <path d=" M 10 30 l 40 0 " fill="transparent" stroke="black"/>        # a line on the page
          <path d=" M 10 40 l 40 0 " fill="transparent" stroke="black"/>        # a line on the page
          <path d=" M 10 50 l 40 0 " fill="transparent" stroke="black"/>        # a line on the page
          <path d=" M 5 0 l 50 0 l 0 10 l -50 0 l 0 -10 " fill="url(#gradient)"/>
          Corpus Icon
        </svg>
        <strong>Corpus</strong>
      </div>
      <div class="col-md protectedbtn" data-href="/profile">
        <svg width="60" height="60" xmlns="http://www.w3.org/2000/svg">
          <defs>
            <linearGradient id="gradient" x1="0%" y1="0%" x2="0%" y2="100%">
              <stop offset="0%" style="stop-color:rgb(0, 0, 0); stop-opacity:1" />
              <stop offset="100%" style="stop-color:rgb(150, 150, 150); stop-opacity:1" />
            </linearGradient>
          </defs>
          <circle cx="30" cy="18" r="18" fill="url(#gradient)"/>                # head
          <circle cx="30" cy="60" r="30" fill="transparent" stroke="black"/>    # body
          Profile Icon
        </svg>
        <strong>My Profile</strong>
      </div>
    </div>
  </div>
</div>
