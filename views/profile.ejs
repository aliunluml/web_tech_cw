<div class = "main_section">

  <% include ../partials/protected-page-banner %>

  <h2 class="page_description">
    My Profile
  </h2>

  <p>You can view your registered information and a list of your posts here.</p>

  <div>
    <ul>
      <% corpusFeed.forEach((user) => { %>
      <li>
        <%= user.username %><br>
        <ol>
          <% user.posts.forEach((post) => { %>
          <li>
            <%= post.content %><br>
            Likes: <%= post.likes %><br>
            Dislikes: <%= post.dislikes %><br>
          </li>
          <% }); %>
        </ol>
      </li>
      <% }); %>
    <ul>
  </div>


</div>
